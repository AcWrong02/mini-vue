#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/registry.npmmirror.com+rollup@4.0.2/node_modules/rollup/dist/bin/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/registry.npmmirror.com+rollup@4.0.2/node_modules/rollup/dist/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/registry.npmmirror.com+rollup@4.0.2/node_modules/rollup/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/registry.npmmirror.com+rollup@4.0.2/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules:/mnt/e/Github/mini-vue/node_modules:/mnt/e/Github/node_modules:/mnt/e/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/registry.npmmirror.com+rollup@4.0.2/node_modules/rollup/dist/bin/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/registry.npmmirror.com+rollup@4.0.2/node_modules/rollup/dist/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/registry.npmmirror.com+rollup@4.0.2/node_modules/rollup/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/registry.npmmirror.com+rollup@4.0.2/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules/.pnpm/node_modules:/mnt/e/Github/mini-vue/mini-vue/node_modules:/mnt/e/Github/mini-vue/node_modules:/mnt/e/Github/node_modules:/mnt/e/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
